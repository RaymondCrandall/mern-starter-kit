{
  "name": "my-js-dev-env",
  "version": "1.0.3",
  "description": "JavaScript Development Environment",
  "scripts": {
    "prestart": "set DEBUG=app:* & babel-node build/startMessage",
    "start": "set DEBUG=app:* & npm-run-all --parallel security-check open-src lint-watch test-watch start-mockapi",
    "security-check": "nsp check",
    "open-src": "nodemon build/srcServer --exec babel-node",
    "lint": "esw  src  build --color",
    "lint-watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress build/testSetup.js \"test/**/*.test.js\"",
    "test-watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node build/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001"
  },
  "author": "Talima",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.0",
    "axios": "^0.18.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "chalk": "^2.4.1",
    "cookie-parser": "^1.4.3",
    "debug": "^4.0.1",
    "express": "^4.16.3",
    "express-compile-sass": "^4.0.0",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "jquery": "^3.3.1",
    "json-schema-faker": "^0.5.0-rc9",
    "mongodb": "^3.1.6",
    "morgan": "^1.9.1",
    "mssql": "^4.2.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "webpack": "^4.17.2",
    "webpack-dev-middleware": "^3.1.3",
    "whatwg-fetch": "^2.0.4",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "ejs": "^2.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-watch": "^3.1.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.11.0",
    "json-server": "^0.14.0",
    "localtunnel": "^1.9.0",
    "mini-css-extract-plugin": "^0.4.2",
    "mocha": "^5.2.0",
    "nock": "^9.3.3",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pug": "^2.0.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "surge": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack-hot-middleware": "^2.23.1",
    "webpack-md5-hash": "0.0.6"
  }
}
